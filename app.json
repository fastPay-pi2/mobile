{
  "expo": {
    "name": "fastPay",
    "scheme": "fastpay",
    "slug": "fastPayApp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/carshier_Logo_cor.png",
    "splash": {
      "image": "./src/assets/images/carshier_Logo_cor.png",
      "backgroundColor": "#fad3b0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "",
    "android": {
      "package": "com.projetointegrador.fastpay",
      "versionCode": 1,
      "permissions": ["CAMERA"],
      "icon": "./src/assets/images/carshier_Logo_cor.png"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
    }
  }
}
